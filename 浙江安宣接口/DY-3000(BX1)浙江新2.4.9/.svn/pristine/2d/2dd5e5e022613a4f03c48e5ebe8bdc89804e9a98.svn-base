<Window x:Class="AIO.xaml.Dialog.Add"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Add" Height="300" Width="565">
    <Grid>
        <DataGrid x:Name="grd" 
              ItemsSource="{Binding StuList}" AutoGenerateColumns="False"
              InitializingNewItem="DataGrid_InitializingNewItem"
              PreviewKeyUp="grd_PreviewKeyUp"
              Loaded="grd_Loaded" Margin="0,44,0,0">
            <DataGrid.Columns>
                <!--<DataGridTextColumn Binding="{Binding Name}" Header="姓名"/>
                <DataGridTextColumn Binding="{Binding Addr}" Header="地址"/>-->
                <DataGridTextColumn Binding="{Binding name}" Header="样品名称"/>
                <DataGridTextColumn Binding="{Binding bianma}" Header="样品编号"/>
                <DataGridTextColumn Binding="{Binding jcmingc}" Header="检测项目名称"/>
                <DataGridTextColumn Binding="{Binding hegez}" Header="检测合格值"/>
                <DataGridTextColumn Binding="{Binding biaozz}" Header="检测标准值"/>
                <DataGridTextColumn Binding="{Binding biaozmc}" Header="检测标准名称"/>
                <DataGridTextColumn Binding="{Binding fh}" Header="判定符号"/>
            </DataGrid.Columns>
        </DataGrid>
        <Button Content="Button" Height="23" HorizontalAlignment="Left" Margin="114,17,0,0" Name="Save" VerticalAlignment="Top" Width="75" Click="Save_Click" />
    </Grid>
</Window>
