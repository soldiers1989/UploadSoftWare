<?xml version="1.0"?>
<configuration>
  <configSections>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
      <section name="AIO.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    </sectionGroup>
  </configSections>
  <connectionStrings>
    <add name="AIO.Properties.Settings.localConnectionString" connectionString="Provider=Microsoft.Jet.OLEDB.4.0;Data Source=|DataDirectory|\bin\Debug\Data\local.Mdb;Persist Security Info=True"
      providerName="System.Data.OleDb" />
  </connectionStrings>
  <startup>

    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
  <appSettings>
    <!--箱仪一体机-->
    
    <!--AD端口-->
    <add key="ADPORT" value="COM3"/>
    <!--摄像头-->
    <add key="SXT1PORT" value="COM4"/>
    <add key="SXT2PORT" value="COM5"/>
    <add key="SXT3PORT" value="COM6"/>
    <add key="SXT4PORT" value="COM7"/>
    <!--打印端口-->
    <add key="PRINTPORT" value="COM8"/>
    <!--通道个数-->
    <add key="HOLECOUNT" value="16"/>
    <!--摄像头个数-->
    <add key="SXTCOUNT" value="2"/>
    <!--重金属个数-->
    <add key="HMCOUNT" value="1"/>
    <!--启用ATP或者微生物-->
    <add key="EnableWswOrAtp" value="1"/>
    <!--启用薄层色谱-->
    <add key="EnableBcsp" value="0"/>
    <!--微生物orATP-->
    <add key="IsWswOrAtp" value="ATP"/>
    <!--微生物软件存放路径-->
    <add key="MICROBIALADDRESS" value=""/>
    <!--是否启用视频播放-->
    <add key="EnableVideo" value="1"/>
    <!--是否启用电池电量显示-->
    <add key="EnableBattery" value="0"/>
    <!--是否启用知识库-->
    <add key="EnableKnowledgeBase" value="1"/>
    <!--是启用演示专用章-->
    <add key="EnableChapter" value="0"/>
    <!--是否启用GPS功能-->
    <add key="EnableGPS" value="0"/>
    <!--是否打开文件夹-->
    <add key="IsOpenFile" value="1"/>
    <!--是否显示测试值-->
    <add key="IsShowValue" value="0"/>
    <add key="FAULTDETECTION" value="1"/>
    <add key="SHOWFGD" value="0"/>
    <add key="Version" value="XZ"/>
    <!--是否显示项目顺序调整-->
    <add key="IsSettingIndex" value="1"/>
    <!--视频路径-->
    <add key="VideoAddress" value="Video\"/>
    <!--pdf路径-->
    <add key="PdfAddress" value="D:\pdf\"/>
    <!--是否允许删除检测结果-->
    <add key="IsDeleted" value="0"/>
    <!--是否启用滤光片测试-->
    <add key="TEST" value="0"/>
    <add key="PrintType" value=""/>
    <!--是否允许修改检测结果-->
    <add key="UpdateCheckedValue" value="1"/>
    <add key="STANDARD1" value="0.2994"/>
    <add key="STANDARD2" value="0.3008"/>
    <add key="STANDARD3" value="0.2609"/>
    <add key="STANDARD4" value="0.2534"/>
    <add key="DECISIONCRITERIA1" value="0.02"/>
    <add key="DECISIONCRITERIA2" value="0.01"/>
    <add key="InstrumentName" value="便携式食品综合分析仪"/>
    <add key="InstrumentNameModel" value="DY-3000(BX1)"/>
    <!--默认为达元接口，AH=安徽 ZH=广东省智慧云平台 GS=甘肃-->
    <add key="InterfaceType" value="DY"/>
    <!--数据库版本信息-->
    <add key="DataBaseVersion" value="1"/>
    <!--分光模块是否在运行-->
    <add key="FgIsStart" value="0"/>
    <!--是否显示电池电量-->
    <add key="IsShowBatteryNum" value="1"/>

    <!--广东省智慧云平台相关-->
    <!--获取快检单号URL-->
    <add key="getsampleURL" value="https://i.gdfda.gov.cn/inspectinfo/jsp/inspect/quick_inspect.jsp?method=getsample"/>
    <!--快检结果上报URL-->
    <add key="uploadResultURL" value="https://i.gdfda.gov.cn/inspectinfo/jsp/inspect/quick_inspect.jsp?method=inspectResult"/>
    <!--快检设备开关机状态上报URL-->
    <add key="deviceStatusURL" value="https://i.gdfda.gov.cn/inspectinfo/jsp/inspect/quick_inspect.jsp?method=deviceStatus"/>
    <!--通过选择快检单号时是否自动选择对应样品名称-->
    <add key="IsSelectSampleName" value="1"/>
    <!--快检结果下载URL-->
    <!--<add key="downloadResultURL" value="http://192.168.19.43:6093/gdfda/quick_inspect.jsp?method=downloadResult"/>
    --><!--快检单上传URL--><!--
    <add key="uploadSampleURL" value="http://192.168.19.43:6093/gdfda/quick_inspect.jsp?method=uploadSample"/>-->

    <!--<add key="WSWPATH" value="Z:\收到的文件\达元\新建文件夹2015\RVLM 2012-04-13 Software 32bit\RVLM 2012-04-13 Software 32bit\RVLM20120413.jar"/>
    <add key="GZZPATH" value="C:\Program Files (x86)\达元一体化食品安全检测系统\FoodClient.exe"/>-->
    <!--<add key="ClientSettingsProvider.ServiceUri" value=""/>-->
  </appSettings>
  <system.web>
    <membership defaultProvider="ClientAuthenticationMembershipProvider">
      <providers>
        <add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri=""/>
      </providers>
    </membership>
    <roleManager defaultProvider="ClientRoleProvider" enabled="true">
      <providers>
        <add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400"/>
      </providers>
    </roleManager>
  </system.web>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="dataSyncHttpBinding" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://124.42.240.136:6089/ccfda/services/dataSync"
        binding="basicHttpBinding" bindingConfiguration="dataSyncHttpBinding"
        contract="NewInterface.dataSyncPortType" name="dataSyncHttpPort" />
    </client>
  </system.serviceModel>
  <applicationSettings>
    <AIO.Properties.Settings>
      <setting name="DY_Detector_NewInterface_dataSync" serializeAs="String">
        <value>http://124.42.240.136:6089/sacmfda/services/dataSync</value>
      </setting>
    </AIO.Properties.Settings>
  </applicationSettings>
</configuration>
