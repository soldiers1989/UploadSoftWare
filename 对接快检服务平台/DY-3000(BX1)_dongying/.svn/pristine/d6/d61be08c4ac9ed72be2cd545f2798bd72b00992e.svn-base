<Window x:Class="AIO.xaml.Main.VersionInfo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="版本信息" Height="400" Width="500" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" 
        Icon="/DY-Detector;component/assets/logo_kjfw.png" Loaded="Window_Loaded">

    <Grid>
        <GroupBox Header="软件信息" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="155" Width="474">
            <RichTextBox IsReadOnly="True" Name="Txt_SoftwareInfo" HorizontalAlignment="Left" Height="130" VerticalAlignment="Top" Width="464" Margin="0,0,-2,0">
                <FlowDocument>
                    <Paragraph>
                        <Run Text=""/>
                    </Paragraph>
                </FlowDocument>
            </RichTextBox>
        </GroupBox>

        <GroupBox Header="硬件信息" HorizontalAlignment="Left" Margin="10,170,0,0" VerticalAlignment="Top" Height="155" Width="474">
            <RichTextBox Name="Txt_HardwareInfo" HorizontalAlignment="Left" Height="130" VerticalAlignment="Top" Width="464" Margin="0,0,-2,0" IsReadOnly="True">
                <FlowDocument>
                    <Paragraph>
                        <Run Text=""/>
                    </Paragraph>
                </FlowDocument>
            </RichTextBox>
        </GroupBox>

        <Button Content="确定" Name="Btn_Closed" Margin="209,331,210,15" Click="Btn_Closed_Click"/>

    </Grid>
</Window>