<Window x:Class="AIO.xaml.Record.PrintPreview"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="打印预览" Height="470" Width="325"
        WindowStartupLocation="CenterScreen" 
        ResizeMode="NoResize" Loaded="Window_Loaded"
        Icon="/DY-Detector;component/assets/logo_v.png">
    <Grid>
        <Label Name="LbCheckItem" Content="农药残留" FontSize="20" HorizontalAlignment="Left" Margin="36,10,0,0" VerticalAlignment="Top" Width="274" HorizontalContentAlignment="Center"/>
        <Label Visibility="Collapsed" Content="被检单位：XXX农贸市场" HorizontalAlignment="Left" Margin="36,80,0,0" VerticalAlignment="Top"/>
        <Label Content="--------------------------------------------------------" HorizontalAlignment="Left" Margin="36,196,0,0" VerticalAlignment="Top" Width="270"/>
        <Label Content="--------------------------------------------------------" HorizontalAlignment="Left" Margin="36,276,0,0" VerticalAlignment="Top" Width="270"/>
        <Label Content="编号          名称          检测结果          判定结果" HorizontalAlignment="Left" Margin="36,221,0,0" VerticalAlignment="Top"/>
        <Label Content="0001         花菜          20.36               合格" HorizontalAlignment="Left" Margin="36,251,0,0" VerticalAlignment="Top"/>
        <CheckBox Name="Cb_IsInstrumentName" IsChecked="True" Content="检测设备：DY-3500Plus(I)食品综合分析仪" HorizontalAlignment="Left" Margin="10,56,0,0" VerticalAlignment="Top" />
        <CheckBox Visibility="Collapsed" Name="Cb_IsCompany" IsChecked="True" Content="" HorizontalAlignment="Left" Margin="10,86,0,0" VerticalAlignment="Top"/>
