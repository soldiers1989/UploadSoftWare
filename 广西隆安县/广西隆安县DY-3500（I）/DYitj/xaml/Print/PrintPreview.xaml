        <CheckBox Name="Cb_IsReviewers" IsChecked="True" Content="审核人员：admin" HorizontalAlignment="Left" Margin="11,341,0,0" VerticalAlignment="Top"/>
        <CheckBox Name="Cb_IsPrintCode" IsChecked="True" Content="打印二维码" HorizontalAlignment="Left" Margin="11,371,0,0" VerticalAlignment="Top"/>
        <Button Name="Btn_Return" Content="恢复默认" HorizontalAlignment="Left" Margin="36,398,0,0" VerticalAlignment="Top" Width="75" Click="Btn_Return_Click"/>
        <Button Name="Btn_Save" Content="保存格式" HorizontalAlignment="Left" Margin="210,398,0,0" VerticalAlignment="Top" Width="75" Click="Btn_Save_Click"/>

